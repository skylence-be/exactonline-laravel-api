{
    "name": "skylence/exactonline-laravel-api",
    "description": "This is my package exactonline-laravel-api",
    "keywords": [
        "skylence",
        "laravel",
        "exactonline-laravel-api"
    ],
    "homepage": "https://github.com/skylence/exactonline-laravel-api",
    "license": "MIT",
    "authors": [
        {
            "name": "Jonas Vanderhaegen",
            "email": "jonas.vanderhaegen@skylence.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.3|^8.4",
        "illuminate/contracts": "^11.0|^12.0",
        "spatie/laravel-package-tools": "^1.16"
    },
    "require-dev": {
        "larastan/larastan": "^3.4",
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^8.1.1",
        "orchestra/testbench": "^10.0",
        "pestphp/pest": "^4.0",
        "pestphp/pest-plugin-arch": "^4.0",
        "pestphp/pest-plugin-laravel": "^4.0",
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpstan-deprecation-rules": "^2.0",
        "phpstan/phpstan-phpunit": "^2.0",
        "spatie/laravel-ray": "^1.35"
    },
    "autoload": {
        "psr-4": {
            "Skylence\\ExactonlineLaravelApi\\": "src/",
            "Skylence\\ExactonlineLaravelApi\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Skylence\\ExactonlineLaravelApi\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Skylence\\ExactonlineLaravelApi\\ExactonlineLaravelApiServiceProvider"
            ],
            "aliases": {
                "ExactonlineLaravelApi": "Skylence\\ExactonlineLaravelApi\\Facades\\ExactonlineLaravelApi"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
